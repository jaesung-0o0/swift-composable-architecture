{"kind":"symbol","identifier":{"interfaceLanguage":"swift","url":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/DependencyValues\/urlSession"},"sections":[],"variants":[{"paths":["\/documentation\/composablearchitecture\/dependencyvalues\/urlsession"],"traits":[{"interfaceLanguage":"swift"}]}],"schemaVersion":{"patch":0,"minor":3,"major":0},"hierarchy":{"paths":[["doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/DependencyValues"]]},"metadata":{"modules":[{"name":"ComposableArchitecture","relatedModules":["Dependencies"]}],"extendedModule":"Dependencies","symbolKind":"property","fragments":[{"kind":"keyword","text":"var"},{"text":" ","kind":"text"},{"kind":"identifier","text":"urlSession"},{"kind":"text","text":": "},{"preciseIdentifier":"c:objc(cs)NSURLSession","text":"URLSession","kind":"typeIdentifier"}],"roleHeading":"Instance Property","externalID":"s:12Dependencies16DependencyValuesV10urlSessionSo12NSURLSessionCvp","title":"urlSession","role":"symbol"},"abstract":[{"text":"The URL session that features should use to make URL requests.","type":"text"}],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"urlSession","kind":"identifier"},{"kind":"text","text":": "},{"preciseIdentifier":"c:objc(cs)NSURLSession","text":"URLSession","kind":"typeIdentifier"},{"text":" { ","kind":"text"},{"text":"get","kind":"keyword"},{"kind":"text","text":" "},{"text":"set","kind":"keyword"},{"text":" }","kind":"text"}],"languages":["swift"]}]},{"content":[{"level":2,"type":"heading","text":"Discussion","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"By default, the session returned from ","type":"text"},{"code":"URLSession.shared","type":"codeVoice"},{"type":"text","text":" is supplied. When used in tests,"},{"type":"text","text":" "},{"text":"access will call to ","type":"text"},{"code":"XCTFail","type":"codeVoice"},{"type":"text","text":" when invoked, unless explicitly overridden:"}]},{"syntax":"swift","code":["\/\/ Provision model with overridden dependencies","let model = withDependencies {","  let mockConfiguration = URLSessionConfiguration.ephemeral","  mockConfiguration.protocolClasses = [MyMockURLProtocol.self]","  $0.urlSession = URLSession(configuration: mockConfiguration)","} operation: {","  FeatureModel()","}","","\/\/ Make assertions with model..."],"type":"codeListing"},{"anchor":"API-client-dependencies","text":"API client dependencies","level":3,"type":"heading"},{"type":"paragraph","inlineContent":[{"text":"While it is possible to use this dependency value from more complex dependencies, like API","type":"text"},{"type":"text","text":" "},{"type":"text","text":"clients, we generally advise against "},{"type":"emphasis","inlineContent":[{"text":"designing","type":"text"}]},{"type":"text","text":" a dependency around a URL session. Mocking"},{"text":" ","type":"text"},{"text":"a URL sessionâ€™s responses is a complex process that requires a lot of work that can be","type":"text"},{"text":" ","type":"text"},{"text":"avoided.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"For example, instead of defining your dependency in a way that holds directly onto a URL"},{"text":" ","type":"text"},{"type":"text","text":"session in order to invoke it from a concrete implementation:"}]},{"syntax":"swift","type":"codeListing","code":["struct APIClient {","  let urlSession: URLSession","","  func fetchProfile() async throws -> Profile {","    \/\/ Use URL session to make request","  }","","  func fetchTimeline() async throws -> Timeline { \/* ... *\/ }","  \/\/ ...","}"]},{"type":"paragraph","inlineContent":[{"text":"Define your dependency as a lightweight ","type":"text"},{"type":"emphasis","inlineContent":[{"type":"text","text":"interface"}]},{"text":" that holds onto endpoints that can be","type":"text"},{"type":"text","text":" "},{"text":"individually overridden in a lightweight fashion:","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["struct APIClient {","  var fetchProfile: () async throws -> Profile","  var fetchTimeline: () async throws -> Timeline","  \/\/ ...","}"]},{"inlineContent":[{"type":"text","text":"Then, you can extend this type with a live implementation that uses a URL session under the"},{"type":"text","text":" "},{"text":"hood:","type":"text"}],"type":"paragraph"},{"syntax":"swift","code":["extension APIClient: DependencyKey {","  static var liveValue: APIClient {","    @Dependency(\\.urlSession) var urlSession","","    return Self(","      fetchProfile: {","        \/\/ Use URL session to make request","      }","      fetchTimeline: { \/* ... *\/ },","      \/\/ ...","    )","  }","}"],"type":"codeListing"}],"kind":"content"}],"references":{"doc://ComposableArchitecture/documentation/ComposableArchitecture/DependencyValues":{"url":"\/documentation\/composablearchitecture\/dependencyvalues","navigatorTitle":[{"text":"DependencyValues","kind":"identifier"}],"kind":"symbol","abstract":[{"text":"A collection of dependencies that is globally available.","type":"text"}],"role":"symbol","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/DependencyValues","title":"DependencyValues","type":"topic","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"DependencyValues"}]},"doc://ComposableArchitecture/documentation/ComposableArchitecture":{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture","role":"collection","url":"\/documentation\/composablearchitecture","abstract":[{"text":"The Composable Architecture (TCA, for short) is a library for building applications in a consistent","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"and understandable way, with composition, testing, and ergonomics in mind. It can be used in"},{"type":"text","text":" "},{"text":"SwiftUI, UIKit, and more, and on any Apple platform (iOS, macOS, tvOS, and watchOS).","type":"text"}],"type":"topic","kind":"symbol","title":"ComposableArchitecture"},"doc://ComposableArchitecture/documentation/ComposableArchitecture/DependencyValues/urlSession":{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/DependencyValues\/urlSession","role":"symbol","url":"\/documentation\/composablearchitecture\/dependencyvalues\/urlsession","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"urlSession","kind":"identifier"},{"kind":"text","text":": "},{"preciseIdentifier":"c:objc(cs)NSURLSession","text":"URLSession","kind":"typeIdentifier"}],"abstract":[{"type":"text","text":"The URL session that features should use to make URL requests."}],"type":"topic","kind":"symbol","title":"urlSession"}}}