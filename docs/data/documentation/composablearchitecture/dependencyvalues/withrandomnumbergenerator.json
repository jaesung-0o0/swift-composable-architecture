 supplying a seeded random number generator as a dependency:"}]},{"syntax":"swift","type":"codeListing","code":["func testRoll() {","  let model = withDependencies {","    $0.withRandomNumberGenerator = WithRandomNumberGenerator(LCRNG(seed: 0))","  } operation: {","    GameModel()","  }","","  model.rollDice()","  XCTAssert(model.dice == (1, 3))","}"]}],"kind":"content"}],"kind":"symbol","metadata":{"fragments":[{"kind":"keyword","text":"var"},{"text":" ","kind":"text"},{"text":"withRandomNumberGenerator","kind":"identifier"},{"kind":"text","text":": "},{"preciseIdentifier":"s:12Dependencies25WithRandomNumberGeneratorC","text":"WithRandomNumberGenerator","kind":"typeIdentifier"}],"roleHeading":"Instance Property","modules":[{"relatedModules":["Dependencies"],"name":"ComposableArchitecture"}],"symbolKind":"property","role":"symbol","extendedModule":"Dependencies","externalID":"s:12Dependencies16DependencyValuesV25withRandomNumberGeneratorAA04WithefG0Cvp","title":"withRandomNumberGenerator"},"identifier":{"url":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/DependencyValues\/withRandomNumberGenerator","interfaceLanguage":"swift"},"sections":[],"hierarchy":{"paths":[["doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/DependencyValues"]]},"references":{"doc://ComposableArchitecture/documentation/ComposableArchitecture/WithRandomNumberGenerator":{"kind":"symbol","type":"topic","title":"WithRandomNumberGenerator","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"WithRandomNumberGenerator"}],"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/WithRandomNumberGenerator","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"WithRandomNumberGenerator"}],"url":"\/documentation\/composablearchitecture\/withrandomnumbergenerator","abstract":[{"type":"text","text":"A dep